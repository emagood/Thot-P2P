[autoload]

GDSync="*res://addons/GD-Sync/MultiplayerClient.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=512

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://class/": "red",
"res://red/": "orange",
"res://thot/": "red"
}

[rendering]

renderer/rendering_method="mobile"
